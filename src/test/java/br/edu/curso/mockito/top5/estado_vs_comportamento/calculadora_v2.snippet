@Test public void soma() { 
	assertEquals(3, calculadora.add(2, 1)); 
	assertEquals(2, calculadora.add(2, 0)); 
	assertEquals(1, calculadora.add(2, -1)); 
}
public class Calculadora {
  	public int soma(int a, int b) {
    	Otimizador otimo = Factory.getOtimizador();
    	return otimo.somar(a + b);
  	}
}